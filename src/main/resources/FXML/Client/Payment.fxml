<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jmc.AutoSalon.Controllers.Client.ClientPaymentController">
   <Label text="Account Name" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="50.0" />
   <TextField fx:id="acc_name" layoutX="162.0" layoutY="46.0" promptText="Name Surname" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="46.0" />
   <Label layoutX="50.0" layoutY="109.0" text="Bank Number: " />
   <Button fx:id="submit_btn" layoutX="165.0" layoutY="215.0" mnemonicParsing="false" onAction="#onSubmitPayment" prefHeight="26.0" prefWidth="70.0" text="Submit" />
   <TextField fx:id="bank_number" layoutX="160.0" layoutY="105.0" promptText="XXXX-YYYY-ZZZZ-VVVV" />
   <Label layoutX="97.0" layoutY="163.0" text="CVV:" />
   <TextField fx:id="cvv_text" layoutX="160.0" layoutY="159.0" promptText="XYZ" />
</AnchorPane>
